{
  "name": "viteapp",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Alessandro Buonerba"
  },
  "description": "Starter pack using Vite, React, Typescript, Chakra and Cypress",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "chakra"
  ],
  "repository": {
    "url": "https://github.com/Dieman89/vite-reactts-chakra-starter"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "tsc && yarn dev",
    "test": "concurrently \"yarn start \" \"cypress open\"",
    "test:ci": "cypress run --headless",
    "gen:dev": "graphql-codegen --config codegen.dev.yml",
    "gen:prod": "graphql-codegen --config codegen.prod.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.4.12",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.1.6",
    "@graphql-codegen/typescript-react-apollo": "^3.1.4",
    "apollo-upload-client": "^16.0.0",
    "axios": "^0.21.4",
    "concurrently": "^6.0.0",
    "framer-motion": "^3.7.0",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.4.1",
    "moment": "^2.29.1",
    "react": "^17.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.0",
    "react-icons": "^4.2.0",
    "react-mapbox-gl": "4.8.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "react-toastify": "^8.0.3",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@types/apollo-upload-client": "^14.1.0",
    "@types/history": "^4.7.9",
    "@types/lodash": "^4.14.173",
    "@types/mapbox-gl": "^2.4.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.5",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.3.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "typescript": "^4.1.2",
    "vite": "^2.0.1",
    "vite-aliases": "^0.5.0"
  }
}
